{
  "compilerOptions": {
    "target": "es5",
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "strictNullChecks": true,
    "module": "commonjs",
    "sourceMap": true,
    "rootDir": "src",
    "outDir": ".",
    "declaration": true,
    "lib": ["esnext", "dom"]
  },
  "exclude": [
    "inputs/**",
    "methods/**",
    "models/**",
    "outputs/**",
    "./*.ts"
  ]
}
